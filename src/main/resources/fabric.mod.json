{
  "schemaVersion": 1,
  "id": "multiconnect",
  "version": "${version}",

  "name": "MultiConnect",
  "description": "This mod allows you to connect to servers on multiple Minecraft versions",
  "authors": [
    "Earthcomputer",
    "Icon made by Vectors Market from Flaticon"
  ],
  "contact": {
    "homepage": "https://earthcomputer.net/",
    "sources": "https://github.com/Earthcomputer/multiconnect",
    "issues": "https://github.com/Earthcomputer/multiconnect/issues"
  },

  "license": "MIT",
  "icon": "assets/multiconnect/icon.png",

  "environment": "client",
  "mixins": [
    "multiconnect.mixins.json",
    "multiconnect.1_15_2.mixins.json",
    "multiconnect.1_14_4.mixins.json",
    "multiconnect.1_14_3.mixins.json",
    "multiconnect.1_14_1.mixins.json",
    "multiconnect.1_14.mixins.json",
    "multiconnect.1_13_2.mixins.json",
    "multiconnect.1_12_2.mixins.json",
    "multiconnect.1_12_1.mixins.json",
    "multiconnect.1_12.mixins.json"
  ],

  "entrypoints": {
    "main": [
      "net.earthcomputer.multiconnect.api.Initializer"
    ]
  },

  "depends": {
    "fabricloader": ">=0.7.5"
  },

  "custom": {
    "modmenu:clientsideOnly": true
  }
}
